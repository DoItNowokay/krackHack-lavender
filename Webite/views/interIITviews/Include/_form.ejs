<form action="/interIIT/api/addOneItem" method="POST" id="add_user">
  <div class="new_user">
    <div class="form-group">
      <label for="Edition" class="text-light">Age</label>
      <input type="hidden" name="id" value="" />
      <input type="number" name="edition" value="" placeholder="" />
    </div>
    <div class="form-group">
      <label for="Medal" class="text-light">Name</label>
      <input type="text" name="medal" value="" placeholder="" />
    </div>
    <div class="form-group">
      <label for="PS" class="text-light">Organ</label>
      <input type="text" name="ps" value="" placeholder="" />
    </div>
    <div class="form-group">
      <label for="Team" class="text-light">Blood Group</label>
      <input type="text" name="team" value="" placeholder="" />
    </div>
    <div class="form-group">
      <label for="UserType" class="text-light">Type User</label>
      <input type="text" name="usertype" value="" placeholder="" />
    </div>

    <div class="form-group">
      <button type="submit" class="btn text-dark update">Save</button>
    </div>
  </div>
</form>
